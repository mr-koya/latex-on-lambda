# texlive.profile written on Fri Jul 26 15:37:03 2024 UTC
# It will NOT be updated and reflects only the
# installation profile at installation time.
selected_scheme scheme-custom
TEXDIR /opt/
TEXMFCONFIG $TEXMFSYSCONFIG
TEXMFHOME $TEXMFLOCAL
TEXMFLOCAL /opt/texmf-local
TEXMFSYSCONFIG /opt/texmf-config
TEXMFSYSVAR /opt/texmf-var
TEXMFVAR $TEXMFSYSVAR
binary_aarch64-linux 1

collection-basic 1
collection-fontsrecommended 1
collection-langenglish 1
collection-langfrench 1
collection-latex 1
collection-latexrecommended 1
collection-mathscience 1
collection-pictures 1
collection-publishers 1

instopt_adjustpath 1
instopt_adjustrepo 1
instopt_letter 1
instopt_portable 1
instopt_write18_restricted 1
tlpdbopt_autobackup 0
tlpdbopt_backupdir /dev/null
tlpdbopt_create_formats 1
tlpdbopt_desktop_integration 0
tlpdbopt_file_assocs 0
tlpdbopt_generate_updmap 1
tlpdbopt_install_docfiles 0
tlpdbopt_install_srcfiles 0
tlpdbopt_post_code 1
tlpdbopt_sys_bin /usr/local/bin
tlpdbopt_sys_info /usr/local/share/info
tlpdbopt_sys_man /usr/local/share/man
tlpdbopt_w32_multi_user 0
